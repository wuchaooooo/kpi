#set($layout ='layout/blank.vm')
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span
            class="sr-only">Close</span></button>
    <h4 class="modal-title" id="myModalLabel">反馈详情</h4>
</div>
<div class="modal-body">
    <textarea id="content" name="content" class="form-control" rows="18" class="form-comment">$!{feedback.content}</textarea>
</div>
<div class="modal-footer">
    <button type="button" id="updateFeedback$!{feedback.id}" onclick="updateFeedback($!{feedback.id})" class="btn btn-primary">保存更改</button>
    <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
</div>



