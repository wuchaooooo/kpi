<div class="row">
    <div class="col-lg-10">

        <table class="table table-bordered table-striped" id="classmate-table">
            <thead>
            <tr>
                <th class="stuID sortable"><a href="#">学号</a><i class="fa fa-sort"></i></th>
                <th class="stuName sortable"><a href="#">姓名</a><i class="fa fa-sort"></i></th>
                <th class="stuPhone sortable"><a href="#">电话</a><i class="fa fa-sort"></i></th>
                <th class="stuEmail sortable"><a href="#">邮箱</a><i class="fa fa-sort"></i></th>
                <th class="status sortable"><a href="#">就业情况</a><i class="fa fa-sort"></i></th>
            </tr>
            </thead>
            <tbody>
                #foreach($!{student} in $!{classmates})
                <tr>
                    <td class="stuID">$!{student.userId}</td>
                    <td class="stuName">$!{student.realName}</td>
                    <td class="stuEmail">$!{student.mobile}</td>
                    <td class="stuPhone">$!{student.email}</td>
                    <td class="status">$!{student.employmentStatus}</td>
                </tr>
